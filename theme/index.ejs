<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>mikefooks.com</title>
  <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro' rel='stylesheet' type='text/css' />
  <link rel="stylesheet" href="main.css" />
</head>
<body>
  <div class="container">
    <div class="title">
      <h1>mike</h1><h1>fooks</h1><h1>.com</h1>
    </div>
    <% posts.forEach(function (post) { %>
      <div class="content">
        <a href="/<%= post.frontmatter.slug %>/"><h3><%= post.frontmatter.title %></h3></a>
      </div>
    <% }); %>
  </div>
</body>
</html>0
